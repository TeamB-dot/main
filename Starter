local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "BlackAir Loader",
   LoadingTitle = "BlackAir Auto Farm",
   LoadingSubtitle = "by BlackAir Team",
})

local Tab = Window:CreateTab("Loader", 4483362458)

Tab:CreateParagraph({
   Title = "Info",
   Content = "Press START to open the Auto Farm panel."
})

Tab:CreateButton({
    Name = "Start",
    Callback = function()
        loadstring(game:HttpGet("YOUR_AUTO_FARM_GUI_LINK"))()
    end,
})
